#import <Foundation/Foundation.h>

@interface s2lsnaps : NSObject

@property (nonatomic, strong) NSNumber *snapId;

@property (nonatomic, assign) NSString *snapname;

@property (nonatomic, assign) NSString *snapdate;

@property (nonatomic, assign) NSString *snapper;

@property (nonatomic, assign) NSString *snappercomment;

@property (nonatomic, assign) NSNumber *snappervote;

@property (nonatomic, assign) NSString *snaplongitude;

@property (nonatomic, assign) NSString *snaplatitude;

@property (nonatomic, assign) NSNumber *snapresult;

- (id)initWithNumber:(NSNumber *)snapId snapname:(NSString *)snapname snapdate:(NSString *)snapdate snapper:(NSString *)snapper snappercomment:(NSString *)snappercomment snappervote:(NSNumber *)snappervote snaplatitude:(NSString *)snaplatitude snaplongitude:(NSString *)snaplongitude  snapresult:(NSNumber *)snapresult;


@end
