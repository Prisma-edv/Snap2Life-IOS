#import "s2lsnaps.h"

@implementation s2lsnaps

@synthesize snapId = _snapId;
@synthesize snapname = _snapname;
@synthesize snapdate = _snapdate;
@synthesize snapper = _snapper;
@synthesize snappercomment = _snappercomment;
@synthesize snappervote = _snappervote;
@synthesize snaplatitude = _snaplatitude;
@synthesize snaplongitude = _snaplongitude;
@synthesize snapresult = _snapresult;

- (id)initWithNumber:(NSNumber *)snapId snapname:(NSString *)snapname snapdate:(NSString *)snapdate snapper:(NSString *)snapper snappercomment:(NSString *)snappercomment snappervote:(NSNumber *)snappervote snaplatitude:(NSString *)snaplatitude snaplongitude:(NSString *)snaplongitude snapresult:(NSNumber *)snapresult;
{
    
    self = [super init];
    
    if (self) {
        self.snapId = snapId;
        self.snapname = snapname;
        self.snapdate = snapdate;
        self.snapper = snapper;
        self.snappercomment = snappercomment;
        self.snappervote = snappervote;
        self.snaplatitude = snaplatitude;
        self.snaplongitude = snaplongitude;
        self.snapresult = snapresult;
    }
    return self;
}
@end
